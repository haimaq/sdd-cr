AddLoadCallback(fn_FormAction6);

function fn_FormAction6(e)
{
  if (condition_CurrentTransition("in", "CRS_CHANGE_REQUESTS.SUBMIT,EPICS.SUBMIT"))
  {
    performSetFieldValue("titleText", "&lt;span nowrap&gt;&lt;b&gt;Submit into:&lt;/b&gt; $SINGULARPROJECTNAME()&lt;/span&gt;", true);
  }
}


function condition_CurrentTransition(operator, value)
{
  var asInternalNames = value.split(',');
  var transitionInternalName = GetTransitionInternalName();
  for (var i = 0; i &lt; asInternalNames.length; i++)
  {
    if (transitionInternalName == asInternalNames[i]) 
    {
      return (operator == "in");
    }
  }
  return (operator != "in");
}

function action_SOOLoadUtils()
{
  SOO.ShellFormatting.init();
}

